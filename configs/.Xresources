! X colors.
! Generated by 'wal'
URxvt*foreground:   #e3e7ed
URxvt*background:   [95]#28353a
URxvt*cursorColor:  #e3e7ed
URxvt*borderColor:  [95]#28353a
URxvt*scrollBar:         false
URxvt*scrollBar_right:   false
URxvt*transparent:       false
URxvt*depth: 32
URxvt.internalBorder: 2
URxvt.externalBorder: 0
URxvt.borderWidth: 0
URxvt.font: xft:Misc Termsynu:size=14:pixelsize=14:style=Regular,xft:Wuncon Siji:pixelsize=14,xft: Hack Nerd Font:style=Medium
URxvt.letterSpace: 0

!*antialias:  true
!*autohint:   true

! URxvt.perl-lib: ~/.urxvt/
URxvt.perl-ext-common: ...,clipboard,...
! URxvt.tabbed.tabbar-fg: 2
! URxvt.tabbed.tabbar-bg: 0
! URxvt.tabbed.tab-fg: 3
! URxvt.tabbed.tab-bg: 2
URxvt.keysym.C-C: perl:clipboard:copy
URxvt.keysym.C-V: perl:clipboard:paste
URxvt.iso14755: false
URxvt.iso14755_52: false

! Colors 0-15.
! special
*.foreground:   #e3e7ed
*.background:   #28353a
*.cursorColor:  #e3e7ed

! black
*.color0:       #2e2e2e
*.color8:       #9ea1a5

! red
*.color9:       #e98ca8
*.color1:       #e27585

! green
*.color10:       #49feed
*.color2:      #7ff0be

! yellow
*.color11:       #e1f4c9
*.color3:      #dfe0a3

! blue
*.color12:       #14aafe
*.color4:      #398cff

! magenta
*.color13:       #fe9df6
*.color5:      #fe79f3

! cyan
*.color14:       #bdd4ff
*.color6:      #7eabff

! white
*.color15:       #e3e7ed
*.color7:      #fbfdff

! Black color that will not be affected by bold highlighting.
*.color66: #151a1e
*color66:  #151a1e

! Xclock colors.
XClock*foreground: #e3e7ed
XClock*background: #151a1e
XClock*majorColor:  rgba:e3/e7/ed/ff
XClock*minorColor:  rgba:e3/e7/ed/ff
XClock*hourColor:   rgba:e3/e7/ed/ff
XClock*minuteColor: rgba:e3/e7/ed/ff
XClock*secondColor: rgba:e3/e7/ed/ff

Xcursor.theme: mycursor

Xft.antialias:  true
Xft.hinting:    true
Xft.hintstyle:  hintfull
Xft.lcdfilter:  lcdlegacy
Xft.rgba:       rgb
