URxvt.background:        [75]#131313
URxvt*scrollBar:         false
URxvt*scrollBar_right:   false

URxvt*transparent:       false

URxvt*depth: 32

URxvt.font: xft:Termsynu:pixelsize=16,xft:Robot Mono Nerd Font:pixelsize=14,xft: Symbola:pixelsize=10,xft:Noto Emoji
URxvt.letterSpace: 0

*antialias:  true
*autohint:   true

! URxvt.perl-lib: ~/.urxvt/
URxvt.perl-ext-common: ...,tabbed,clipboard,...
URxvt.tabbed.tabbar-fg: 2
URxvt.tabbed.tabbar-bg: 0
URxvt.tabbed.tab-fg: 3
URxvt.tabbed.tab-bg: 2

URxvt.keysym.C-C: perl:clipboard:copy
URxvt.keysym.C-V: perl:clipboard:paste
URxvt.iso14755: false
URxvt.iso14755_52: false
